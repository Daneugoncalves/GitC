<Window x:Class="WpfAppEntity.Views.frmEditUser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppEntity.Views"
        mc:Ignorable="d"
        Title="frmEditUser" Height="200" Width="340">
    <Grid>
        <TextBox Name="tbxName" HorizontalAlignment="Left" Height="23" Margin="30,30,0,0" TextWrapping="Wrap" Text="Usuario" VerticalAlignment="Top" Width="271"/>
        <TextBox Name="tbxLogin" HorizontalAlignment="Left" Height="23" Margin="30,60,0,0" TextWrapping="Wrap" Text="Login" VerticalAlignment="Top" Width="271"/>
        <TextBox Name="tbxSenha" HorizontalAlignment="Left" Height="23" Margin="30,80,0,0" TextWrapping="Wrap" Text="Senha" VerticalAlignment="Top" Width="271"/>
        <Button Click="Button_Click" Content="Button" HorizontalAlignment="Left" Margin="30,110,0,0" VerticalAlignment="Top" Width="271" Height="42"/>

    </Grid>
</Window>
